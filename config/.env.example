# Environment variables for Road Photo Capture System
# Copy this file to .env and customize values

# Backend API
BACKEND_API_URL=http://192.168.1.100:8000/api/upload

# GPS Module
GPS_SERIAL_PORT=/dev/ttyUSB0
GPS_BAUDRATE=9600

# Camera
CAMERA_DEVICE=/dev/video0

# System
DEVICE_ID=TERMINAL_001
LOG_LEVEL=INFO
LOG_PATH=/var/log/road-capture/app.log

# Capture Settings
CAPTURE_INTERVAL=1.0
